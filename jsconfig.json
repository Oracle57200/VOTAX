{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "moduleResolution": "node",
    "strict": false,
    "declaration": false,
    "sourceMap": false,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "typeRoots": ["."]
  },
  "include": ["*.js"],
  "exclude": ["node_modules"]
}